{
    "manifest_version": 2,

    "name": "Thanxtension Plugin",
    "description": "This extension will print a thank you page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "rocket.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>",
        "tabs"
    ],
    "content_scripts": [{
        "js": ["content.js"],
        "matches": ["<all_urls>"]
    }]
}